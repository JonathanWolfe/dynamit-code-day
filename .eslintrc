{
	"root": true,
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"extends": [
		"airbnb-base"
	],
	"rules": {
		"no-console": 0,
		"import/no-extraneous-dependencies": [
			"error",
			{
				"devDependencies": true
			}
		],
		"no-tabs": "off",
		"indent": [
			"error",
			"tab"
		]
	}
}
